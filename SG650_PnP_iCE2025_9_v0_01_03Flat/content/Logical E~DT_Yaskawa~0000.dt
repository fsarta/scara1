TYPE
    
    // Enum for general error state
    E_ErrorSeverity : (
        None := 0,          // No active errors
        Info := 1,          // Informational, no impact on operation
        Warning := 2,       // Minor issues, operation can continue
        MinorError := 3,    // An error occurred
        MajorError := 4,    // Critical error
        FatalError := 5     // System critical error
    ) OF UDINT;

    // Structure for a single error entry
    ST_ErrorEntry : STRUCT
        ErrorCode      : UDINT;             // Unique ID of the error
        ErrorSource    : STRING;            // Name of the function block or module where the error originated (e.g., 'MC_MoveAbsolute_1')
        ErrorMessage   : STRING512;         // Descriptive error message
        Severity       : E_ErrorSeverity;   // Severity of this specific error
        IsActive       : BOOL;              // TRUE if the error is currently active
        Timestamp      : Yt_YA_RTC_Struct;  // When the error became active
        Acknowledged   : BOOL;              // TRUE if the error has been acknowledged by an operator
    END_STRUCT;
    
    arrST_ErrorEntry : ARRAY[1..C_ERROR_BUFFER_SIZE] OF ST_ErrorEntry;
    
    ST_ErrorLookupEntry : STRUCT
        ErrorCode : UINT;
        Message : STRING512;
    END_STRUCT
    
    arrST_ErrorLookupEntry : ARRAY[1..C_MAX_ERROR_LOOKUP_ENTRIES] OF ST_ErrorLookupEntry;
    
    ST_GlobalErrorStatus : STRUCT
        bAnyErrorActive     : BOOL;        // TRUE if any error is currently active (IsActive = TRUE)
        udiActiveErrorCount : UDINT;       // Number of currently active errors
        eHighestSeverity    : E_ErrorSeverity; // Highest severity among active errors
        bBufferFull         : BOOL;        // TRUE if the circular buffer is full
        // udiErrorHead        : UDINT;
        // udiErrorTail        : UDINT;
    END_STRUCT;
    
END_TYPE

